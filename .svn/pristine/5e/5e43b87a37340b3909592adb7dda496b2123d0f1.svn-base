DROP TABLE IF EXISTS `llidc_orders`;
CREATE TABLE `llidc_orders` (
  `o_id` int(11) NOT NULL AUTO_INCREMENT,
  `o_ip` varchar(255) NOT NULL COMMENT 'machine''s IP',
  `o_client_id` varchar(255) NOT NULL COMMENT 'client''s id',
  `o_idc_location` varchar(255) NOT NULL COMMENT 'machine room location',
  `o_start_datetime` datetime DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'order valid time',
  `0_expire_datetime` datetime DEFAULT NULL COMMENT 'order expires time',
  `o_payment_status` int(1) NOT NULL DEFAULT '0' COMMENT 'order payment status:0 means  pending,1 means paid',
  `o_handler` varchar(255) DEFAULT NULL COMMENT 'to whom the order belongs ',
  `o_auditor` varchar(255) DEFAULT NULL COMMENT 'person who audits the order',
  `o_memo` varchar(255) DEFAULT NULL COMMENT 'memo of this order',
  PRIMARY KEY (`o_id`),
  KEY `orders` (`o_ip`,`o_client_id`)
