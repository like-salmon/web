}

form.frow div.tips.cr{
    color:#13981e;
}

form .frow label{
    font-size: 1.1em;
    margin-left: 74px;
}

form .frow div.tips {
    margin-top: 23px;
    position: absolute;
    margin-left: 220px;
    color: red;
}

form .frow label{
    font-size: 1.1em;
    margin-left: 74px;
}

form input.finput{
    height: 35px;
    border-radius: 5px;
    margin-left: 20px;
