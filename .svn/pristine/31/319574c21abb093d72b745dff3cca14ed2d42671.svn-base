/*图片模态框*/
.imgmodal {
    position: fixed;
    background-color: #000;
    top: 0;
    left: 0;
    z-index: 9;
    width: 100%;
    height: 100%;
    opacity: 0.7;
}

#mclose,.mclose{
    width: 33px;
    position: absolute;
    z-index: 6;
    top: -15px;
    right: -17px;
}

