    padding:10px 20px;
    margin-bottom: 32px;
    margin-left: 86px;
    width: 440px;
}
form .frow div.tips {
    margin-top: 23px;
    position: absolute;
    margin-left: 220px;
    color: red;
}
form .frow div.tips.cr{
