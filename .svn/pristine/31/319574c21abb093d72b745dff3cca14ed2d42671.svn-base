*{
    margin:0;
    border:0;
    padding:0;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    line-height: 1.4;
}


a{
    color:#00B1FB;
    text-decoration: none;
    transition:all 0.3s ease-in 0.1s;
}

a:hover,a:focus{
    text-decoration: none !important;
    border-bottom: none;
}

a, img {
    border:none;
    outline:none;
}

body{
    font-size: 13px !important;
    background-color: #f7f7f7;
    /*font-family: Arial, "微軟正黑體", "文泉驛正黑", "WenQuanYi Zen Hei", "儷黑 Pro", "LiHei Pro","Microsoft JhengHei", "標楷體", DFKai-SB, sans-serif;*/
    font-family:Microsoft YaHei,SimHei,SimSun;
}

img{
    width: 100%;
}

img.map, map area{
    outline: none;
}

img.map, map area{
    /*outline: none;*/
}

ul li{
    list-style-type: none;
}

ol li{
    list-style-type: decimal;
}

hr {
  border: 0;
  clear:both;
  display:block;
  width: 96%;
  background-color:#ccc;
  height: 1px;
}
.content{
    width:80%;
    margin:0 auto;
}
.loading{
    display: none;
}

/*模态框部分*/
.modal-bg,div[class^="modal-bg"]{
    position: fixed;
    top:0;
    left:0;
    display: none;
    width:100%;
    height:100%;
    z-index: 3;
    background-color: #fff;
    opacity: 0.7;
}

.tmodal{
    position: fixed;
    top:0;
    left:0;
    display: none;
    width:100%;
    height:100%;
    z-index: 9;
    background-color: #555;
    opacity: 0.6;
}

.tipct{
    width:20%;
    min-height: 180px;
    border-radius: 5px;
    position: absolute;
    margin-left: auto;
    margin-right: auto;
    left:0;
    right:0;
    top:20%;
    z-index: 9;
}

.tipct i{
    font-size: 8em;
    display: block;
    margin: 0 auto;
    width: 96px;
}

.tipct p.desc{
text-align: center;
    font-size: 3em;
}

.tipct .tclose{
    width: 33px;
    float: right;
    margin-right: -16px;
    margin-top: -16px;
}

.modal-dt{
    position: relative;
    height: 480px;
    overflow: scroll;
    overflow-x: hidden;
    padding: 20px 50px;
    background-color: #fff;
    overflow: auto;
}

/*模态框标题*/
.modal-ct h2,div[class^="modal-ct"] h2{
    text-align: center;
    padding: 10px 20px;
    color: #fcfcfc;
    border: 1px solid #ccc;
    background-color: #17578d;
    /*background-color: #9ca9b1;*/
}

/* 模态框内容部分*/
div[class^="modal-ct"] .modal-dt{
    background-color: #f7f7f7;
    border:0;
}

.modal-dt ~ div.md-footer{
    position: absolute;
    font-size: 1.3em;
    width: 768px;
    text-align: center;
    background-color: #f7f7f7;
    padding-top: 10px;
    padding-bottom: 10px;
    box-shadow: 0 5px 15px rgba(0,0,0,.5);
}

.modal-dt article p{
    line-height: 30px;
}

.modal-ct,div[class^="modal-ct"]{
    /*width:50%;*/
    max-width: 768px;
    z-index: 4;
    display: none;
    position: fixed;
    top:25%;
    left:0;
    right:0;
    margin-left: auto;
    margin-right: auto;
    background-color: #fff;
    box-shadow: 0 5px 15px rgba(0,0,0,.5);
}

.modal-ct-img{
    top:20%;
    display: block;
    position: fixed;
    left:0;
    right:0;
    margin-left: auto;
    margin-right: auto;
    z-index: 3;
    box-shadow: 5px 5px 5px #c5c4c4;
}
.modal-ct-img img + img,.modal-ct-img{
    width: 600px;
    height: 480px;
}

.modal-ct-cf.hide,div[class^="modal-ct"].hide{
    animation-name:modalhide;
    animation-duration:1s;
    animation-timing-function: ease;
}

/*confirm modal*/
.modal-dt-cf{
    width: 400px;
    height: 120px;
    background-color: #FFF;
    border-radius: 5px;
    /* padding: 10px 20px; */
    box-shadow: 1px 2px 3px #ccc;
    border: 1px solid #f1f0f0;
    z-index: 4;

}
.modal-ct-cf{
    width: 400px;
    height:120px;
    display: none;
    position: absolute;
    top: 30%;
    left:0;
    right:0;
    margin-left: auto;
    margin-right: auto;
    z-index: 3;
}
/* confirm modal title */
.modal-dt-cf h3{
    text-align: center;
    margin-top: 7px;
    border-bottom: 1px solid #f7f7f7;
}
/*confirm modal btn area*/
.modal-dt-cf div.btn{
    text-align: center;
}

.modal-dt-cf div.btn input{
    margin-right: 20px;
    padding: 10px 12px;
    background-color: #c4996c;
    color: #fff;
    margin-top: 20px;
    border-radius: 5px;
}

.modal-bg.active,.modal-ct.active,div[class^="modal-ct"].active,.modal-ct-cf.active,.tmodal.active{
    display: block;
}
.modal-ct.active,div[class^='modal-ct'].active{
        animation-name:modaltop;
    animation-duration:.5s;
    animation-timing-function: ease;
}

@keyframes modaltop {
    from{
        opacity: 0;
        transform: scale(0.5);
    }
    to{
        opacity:1;
        transform: scale(1);
    }
}

@keyframes modalhide {
    0% {
        opacity: 1;
    }
    100%{
        top:-1500px;
        opacity: 0;
    }
}
