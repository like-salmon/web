/* custom check box*/
#smsf .frow input#expire{
    display: none;
}

#smsf .frow label.checkbox{
    display: inline-block;
    text-align: right;
    margin-left: 6px;
        font-size: 28px;
    vertical-align: -6px;
    font: normal normal normal 14px/1 FontAwesome;
    text-rendering: auto;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;

}
#smsf .frow label.checkbox:before{
    content: "\f096";
        font-size: 24px;
}

#smsf .frow input#expire:checked + label.checkbox:before{
    font-size: 24px;
    content: "\f046";
    color:#000;
}


