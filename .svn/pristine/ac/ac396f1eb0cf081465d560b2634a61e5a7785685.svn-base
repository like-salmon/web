{% extends "admin_panel.html" %}
{% block content %}

    <form method="get" id="smsf">
        <h3><i class="fa fa-search fa-lg"></i>搜索订单</h3>
        <div class="frow">
                    <label>服务类型:</label>
        <select name="stype" id="stype">
            <option value="b" {% if stype == 'b'%} selected {% end %}>租用</option>
            <option value="c" {% if stype == 'c'%} selected {% end %}>托管</option>
            <option value="d" {% if stype == 'd'%} selected {% end %}>试用</option>
        </select>
        </div>
        <div class="frow ccb">
            <label>是否过期:</label><input type="checkbox" value="1" name="expire" id="expire" {% if expire %}checked="checked"{% end %}/><label class="checkbox" for="expire"></label>
        </div>
        <div class="area frow">

            <div class="nums">
                <label>客户账号:</label><input type="text" name="u_acc" class="u_acc" value="{{u_acc}}"/>
            </div>
            <div class="nums ip">
                <label>机器IP:</label><input type="text" name="ips" class="ips" value="{{ips}}"/>
            </div>
            <div class="nums">
                <input type="submit" value="搜索"/>
            </div>
        </div>

    </form>
    <h3>订单列表</h3>
    <table class="orders_tb">
        <tbody>

        <tr class="title">
            <td>编号</td>
            <td>客户账号</td>
            <td>机器IP</td>
           {#  {% if ctype in [4,5]%}<td>订单金额</td>{% end %} #}
            <td>出机时间</td>
            <td>到期时间</td>
            <td>订单金额</td>
            <td>产品配置</td>
            <td>业务类型</td>
            <td>备注</td>
            <td>操作</td>
        </tr>

        {% if orders %}
        {% from datetime import datetime %}
        {% for i in orders %}
            <tr class="olist">
                <td class="tid">{{i[0]}}</td>
                <td><input type="text" disabled="disabled" value="{{i[2]}}"/></td>
                <td><input type="text" disabled="disabled" value="{{i[1]}}"/></td>
                {#{% if ctype in [4,5]%}<td><input type="text" disabled="disabled" value="{{i[3] if i[3] else 0}}"/></td>{% end %}#}
                <td><input type="text" disabled="disabled" value="{{ datetime.strftime(i[4],'%Y/%m/%d')}}"/></td>
                <td><input type="text" disabled="disabled" value="{{ datetime.strftime(i[5],'%Y/%m/%d') }}"/></td>
                <td><input type="text" disabled="disabled" value="{{i[3]}}"/></td>
                <td><input type="text" disabled="disabled" value="{{i[31]}}"/></td>
                <td><input type="text" disabled="disabled" value="{{i[29].split(',')[1]}}"/></td>
                <td><input type="text" disabled="disabled" value="{{i[9] if i[9] else ''}}"/></td>
                <td><span class="modify">详情</span>|{% if ctype in [4,5] %}<span class="renew">续期</span>|{% end %}<span class="remove">删除</span></td>
            </tr>
        {% end %}
        {% end %}
        </tbody>
    </table>
    <ul class="pagination">

        {% if pages and 1< pages <= 24 %}
        {% for i in range(1,pages+1) %}
            {% if i == offset %}
                    <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="active">{{i}}</a></li>
                    {% else %}
                    <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="">{{i}}</a></li>
                    {% end %}
            {% end %}
        {% end %}

        {% if pages and pages > 24 %}
        {# ------ #}
        <li><a href="/admin/orders?offset={{1}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class=""><<</a></li>

        {% if offset > 1%}
            <li><a href="/admin/orders?offset={{offset-1}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class=""><</a></li>
        {% end %}
        {% if offset < pages/3 %}
            {% if (pages/3 - offset) >6 and offset >9  %}
               {% for i in range(1,5) %}
                    <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="">{{i}}</a>
                    {% end %}
                    <li><a href="javascript:;" class="empty">...</a>
                    {% for i in range(offset-2,offset+3) %}
                    {% if i == offset %}
                    <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="active">{{i}}</a>
                    {% else %}
                    <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="">{{i}}</a>
                    {% end %}
                    {% end %}
            {% elif (pages/3 - offset) > 6 and offset <=9  %}
                {% for i in range(1,offset+4) %}
                    {% if i == offset %}
                    <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="active">{{i}}</a>
                    {% else %}
                    <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="">{{i}}</a>
                    {% end %}
                {% end %}
        {% elif (pages/3 - offset) <= 6 and offset>9  %}
            {% for i in range(1,5) %}
                    <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="">{{i}}</a>
                    {% end %}
                    <li><a href="javascript:;" class="empty">...</a>
                    {% for i in range(offset-3,offset+3) %}
                    {% if i == offset %}
                    <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="active">{{i}}</a>
                    {% else %}
                    <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="">{{i}}</a>
                    {% end %}
                    {% end %}
        {% elif (pages/3 - offset) <= 6 and offset <= 9  %}
            {% for i in range(1,offset+4) %}
                    {% if i == offset %}
                    <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="active">{{i}}</a>
                    {% else %}
                    <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="">{{i}}</a>
                    {% end %}
                {% end %}
    {% end %}

        <li><a href="javascript:;" class="empty">...</a></li>

        {% for i in range(pages/2-3,pages/2+3) %}
            <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}">{{i}}</a></li>
        {% end %} {# end for #}
        <li><a href="javascript:;" class="empty">...</a></li>

        {% for i in range(pages-3,pages+1) %}
            <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}">{{i}}</a></li>
        {% end %}
        {% end %} {# end if offset 1/3 #}

        {# ------ #}

        {# 1/3 ~ 2/3 part #}
        {% if pages/3 <= offset <= pages/3*2 %}
        {% for i in range(1,5) %}
            <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}">{{i}}</a></li>
        {% end %}{# end for #}

        <li><a href="javascript:;" class="empty">...</a></li>

        {% for i in range(offset-3,offset+4) %}
        {% if i == offset %}
            <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="active">{{i}}</a></li>
            {% else %}
        <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}">{{i}}</a></li>
        {% end %} {# end if #}

        {% end %} {# end for #}
        <li><a href="javascript:;" class="empty">...</a></li>

        {% for i in range(pages-3,pages+1) %}
            <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}">{{i}}</a></li>
        {% end %} {# end for #}
        {% end %} {# end if offset 2/3 #}

        {# ------ #}

        {# pages-3 part #}
        {% if pages/3*2 < offset <= pages %}
        {% for i in range(1,6) %}
            {% if i == offset %}
            <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="active">{{i}}</a></li>
            {% else %}
            <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="">{{i}}</a></li>
            {% end %}
        {% end %}
        <li><a href="javascript:;" class="empty">...</a></li>

        {% for i in range(pages/2-3,pages/2+3) %}
            {% if i == offset %}
            <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="active">{{i}}</a></li>
            {% else %}
            <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="">{{i}}</a></li>
            {% end %}
        {% end %} {# end for #}
        <li><a href="javascript:;" class="empty">...</a></li>

        {% if pages - offset < 6 %}
            {% for i in range(offset-2,pages+1) %}
            {% if i == offset %}
                <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="active">{{i}}</a></li>
            {% else %}
                <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="">{{i}}</a></li>
            {% end %}
            {% end %}
        {% else %}
            {% for i in range(offset-2,offset+2) %}
                {% if i == offset %}
                <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="active">{{i}}</a></li>
                {% else %}
                <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="">{{i}}</a></li>
                {% end %}
            {% end %}
            <li><a href="javascript:;" class="empty">...</a></li>
            {% for i in range(pages-3,pages+1) %}
                {% if i == offset %}
                    <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="active">{{i}}</a></li>
                {% else %}
                    <li><a href="/admin/orders?offset={{i}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="">{{i}}</a></li>
                {% end %}
            {% end %} {# end for #}
        {% end %}
        {% end %} {# end if if 2/3*pages<= offset <= pages% #}
        {% if offset < pages %}
        <li><a href="/admin/orders?offset={{offset+1}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="">> </a></li>
        {% end %}
        <li><a href="/admin/orders?offset={{pages}}&u_acc={{u_acc}}&ips={{ips}}&stype={{stype}}&expire={{expire}}" class="">>></a></li>
        {% end %} {# end if pages #}
    </ul>
<div class="modal-ct modal-ct-s-4">
        <img class="mclose 3" src="{{static_url("img/close.png")}}"/>
        <h2>订单详情</h2>
        <div class="modal-dt">
            <form id="oform" class="mform" action="/admin/orders/">
                {% raw xsrf_form_html() %}
                <div class="data">
                </div>
                <div class="frow" style="text-align: center;">
                    <input type="submit" class="n_addbtn sbtn" value="提交" style="background-color: #17578d;color:#fff;"/>

                    <div class="tips"><span></span></div>
                </div>

            </form>
        </div>

    </div>
{% end %}
