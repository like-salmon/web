form#adlgform {
    width: 550px;
    position: fixed;
    left: 0;
    right: 0;
    margin-left: auto;
    margin-right: auto;
    color: #fff;
    border: 1px solid #fffffe;
    top: 250px;
    border-radius: 5px;
    padding: 15px 25px;
    text-align: center;
    padding-top: 35px;
}
.content.adminlg{
    width:100%;
}
form#adlgform .ccha {
    width: 120px;
    position: absolute;
    margin-top: -2px;
    right: -133px;
}

form#adlgform .ccha img {
    border-radius: 5px;
}

#adlgform .frow input + i.fa {
    position: absolute;
    opacity: 0;
    transition: all 0.1s ease-in;
    left: 0;
    background: #2196F3;
    color: #fff;
    display: block;
    padding: 4px;
}

#adlgform .frow {
    padding: 0;
    margin: unset;
    width: 258px;
    margin: 0 auto;
    display: inline-block;
    position: relative;
}

#adlgform .frow input[type=submit] {
    width: 100px;
    padding: 7px 10px;
    border-radius: 5px;
    background: #564033;
    color: #fff;
}

#adlgform .frow input {
    margin-bottom: 20px;
}

#adlgform .frow input:focus + i.fa {
    opacity: 1;
    left: 25px;
    transition: all 0.25s ease-out;
}

#adlgform form input.finput {
    text-indent: 35px;
}
#adlgform .frow .tips{
margin-top: 34px;
    margin-left: 105px;
    width: 250px;
}

.adminlg {
    background: url("/static/img/admin_bg.jpg") no-repeat center center;
    background-attachment: fixed;
    transition: all 3s ease-in;
    -moz-transition: all 5s ease-in-out;
    -ms-transition: all 5s ease-in-out;
    -webkit-transition: all 5s ease-in-out;
    -o-transition: all 5s ease-in-out;
    width: 100%;
    padding-bottom: 100%; /*fake height*/
}

.adminlg:hover, .adminlg:focus {
    transform: scale(1.1);
    -moz-transform: scale(1.1);
    -webkit-transform: scale(1.1);
    -o-transform: scale(1.1);
    -ms-transform: scale(1.1); /* IE 9 */
    -ms-filter: "progid:DXImageTransform.Microsoft.Matrix(M11=1.1, M12=0, M21=0, M22=1.1, SizingMethod='auto expand')"; /* IE8 */
    filter: progid:DXImageTransform.Microsoft.Matrix(M11=1.1, M12=0, M21=0, M22=1.1, SizingMethod='auto expand'); /* IE6 and 7 */
}

.adminlg .frow .cctips{
    margin-left: 110px;
    width: 199px;
}
#smsf,#osmsf{
    text-align: right;
    margin-bottom: 18px;
}
#smsf input[type="text"],#osmsf input[type="text"]{
    height:30px;
    border-radius: 5px;
    text-indent: 15px;
}
#smsf input[type="submit"],#osmsf input[type="submit"]{
    width: 44px;
    padding: 8px 10px;
    text-align: center;
    border-radius: 5px;
    background-color: #eaa749;
    color: #fff;
}
#smsf #mtype,#osmsf #mtype{
    height: 30px;
}
.pagination li a{
    color: #fff;
    border: 1px solid #dddddd;
    padding: 5px;
    background-color: #afa7a1;
}

.pagination li a.active{
    background-color: #fff;
    color: #555;
}

.pagination{
    text-align: center;
     margin-bottom:65px;
    margin-top: 25px;
}

/** panel part */

#nav .content,#top .right ul li,#nav,.content.panel,#top .left img,#top .left h3,.pagination li{
    display: inline-block;
    *display: inline;
    zoom:1;
    vertical-align: top;
}

#top .right ul {
        margin-top: 18px;
    color: #f2f2f2;
}
#top .left h3 {
    margin-top: 19px;
    margin-left: 12px;
    color: #f2f2f2;
}
#top .left{
    width:249px;
    float: left;
    margin-left: 25px;
}
#top .left img{
    width:49px;
}

#top .right{
    float:right;
    margin-right: 25px;
}
#top{
    background: #17578d;
        width: 100%;
    height: 58px;
}

.content.panel {
    min-height: 630px;
    padding: 12px 15px
}
#nav{
    width:245px;
    min-height: 610px;
    background: #555;
    text-align: center;
    font-size: 1.1em;
    color:#fff;
    margin-top: 4px;
}
#nav ul li {
    padding: 10px 12px;
    text-align: justify;
    padding-left: 52px;
}
#nav ul li.active{
    background:#769f22;
}
#nav ul li a{
    color:#fff;
}
#nav ul li a span{
    padding-left: 15px;
}
/* footer*/
#footer p{
    text-align: center;
    font-size: 1.1em;
    color:#fff;

}

/* all machines */
.machines_tb,.orders_tb{
    width:100%;
    border-spacing: 0 2px;
    border-collapse: collapse;
}
.machines_tb .title,.orders_tb .title{
        /*background-color: #f3e1c8;*/
}
.machines_tb .title td,.orders_tb .title td{
    text-align: center;
    background: #c1d990;
}
.machines_tb tr td,.orders_tb tr td{
    padding: 9px 12px;
    border:1px solid #ccc;
        white-space: nowrap;
    text-overflow: initial;
    overflow: hidden;
    text-align: center;
}
.machines_tb tr td input[type="text"],.orders_tb tr td input[type="text"]{
    background: #f7f7f7;
        text-align: center;
    width: 100%;
}

/* modal form css*/
.mform .frow{
    padding: 7px 16px;
    margin-bottom: 22px;
    margin-left: 109px;
    width: 450px;
    text-align: justify;
        position: relative;
}
.mform .data .frow input{
    background: unset;
    border-bottom: 1px solid #ccc;
    text-indent: 15px;
        margin-left: 175px;
}
.mform .data .frow input:focus{
    outline: none;
}
.mform .data .frow label{
font-size: 1.1em;
    font-weight: 700;
    margin-right: 14px;
    color: #6e6767;

    position: absolute;
}
.mform .data .frow select{
    background: unset;
    padding: 6px 8px;
    margin-left: 168px;
    border: 1px solid #ccc;
}
.mform .frow .sbtn{
    background-color: #ccc;
    color: #fff;
    border-radius: 5px;
    width: 120px;
    padding: 10px 12px;
}
.mform .frow .tips {
    position: absolute;
    top: -48px;
    margin-left: 143px;
}
.mform .frow div.tips span{
    margin-left: 6px;
    margin-top: -12px;
}
.fa-check-circle-o,.fa-check-circle-o ~span{
    color:limegreen;
}
.fa-times-circle-o,.fa-times-circle-o ~span{
    color:red;
}
.fa-info-circle,.fa-info-circle ~span{
    color:#c9c5c5;
}

/* orders*/
.orders_tb tr td span.renew,.orders_tb tr td span.remove,.orders_tb tr td span.modify{
    cursor: pointer;
}

.orders_tb tr td span.renew:hover,.orders_tb tr td span.remove:hover,.orders_tb tr td span.modify:hover{
    color:#0f68d3;
    font-weight: 600;
    transition: color .6s ease-in-out;
}
#oform .frow .otip{
    color:red;
    margin-left: 174px;
    margin-top: 3px;
}

#oform .frow .auditbtn{
    background-color: #eaa749;
    color:#fff;
    margin-left: 29px;
}

.loading{
    display: none;
    position: absolute;
    left: 0;
    right: 0;
    margin-left: auto;
    margin-right: auto;
    width: 25px;
    top: 45%;
    z-index: 9;
    /* margin-left: 20px; */
}

#footer{
    width: 100%;
    height:50px;
    background: #555;
    padding-top: 15px;
    bottom:0;
    font-size: 0.9em;
    position: fixed;
}